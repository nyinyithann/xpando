{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "func-names": "off",
    "no-empty-function": "off",
    "import/extensions": "off",
    "no-underscore-dangle" : "off",
    "max-len": "off",
    "prefer-destructuring" : "off",
    "prefer-rest-params": "off",
    "no-restricted-syntax": "off",
    "no-sparse-arrays": "off"
  }
}

